@using MvcLibrary.Models.Entity
@model List<TBLBOOK>
@{
    ViewBag.Title = "AuthorBooks";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>@ViewBag.name</h2>
<br />
<table class="table table-striped table-bordered">
    <tr style="background-color:rgb(252,178,22)">
        <th>BookID</th>
        <th>Book Name</th>
        <th>Book Category</th>
        <th>Book PrintDate</th>
        <th>Book Publisher</th>
        <th>Book Page</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.TBLCATEGORY.CategoryName</td>
            <td>@item.PrintDate</td>
            <td>@item.Publisher</td>
            <td>@item.Page</td>
        </tr>
    }
</table>
<a href="/Author/Index/" class="btn btn-primary">Return Back</a>
