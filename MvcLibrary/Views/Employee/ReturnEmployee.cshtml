@model MvcLibrary.Models.Entity.TBLEMPLOYEE
@{
    ViewBag.Title = "ReturnEmployee";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("UpdateEmployee", "Employee", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        @Html.HiddenFor(e=>e.ID)
    </div>
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        @Html.LabelFor(e => e.Employee)
        @Html.TextBoxFor(e => e.Employee, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Update</button>
    </div>
}

