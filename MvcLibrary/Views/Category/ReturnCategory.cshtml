@model MvcLibrary.Models.Entity.TBLCATEGORY
@{
    ViewBag.Title = "ReturnCategory";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("UpdateCategory","Category",FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        @Html.HiddenFor(c=>c.ID)
        <br />
        @Html.LabelFor(c=>c.CategoryName)
        @Html.TextBoxFor(c=>c.CategoryName, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Update</button>
    </div>
}

